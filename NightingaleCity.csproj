<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<EnableHarmony>true</EnableHarmony>
		<LangVersion>12.0</LangVersion>

		<MinimumApiVersion>4.1.0</MinimumApiVersion>
		<MinimumGameVersion>1.6.15</MinimumGameVersion>
		<Name>Nightingale City</Name>
		<UniqueId>suzukiPC.NightingaleCityCode</UniqueId>
		<Authors>rokugin, Pau</Authors>
		<Description>Code component for Nightingale City.</Description>
		<UpdateKeys>Nexus:0</UpdateKeys>
		<Version>1.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove=".gitattributes" />
	  <None Remove=".gitignore" />
	  <None Remove="LICENSE" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Leclair.Stardew.ModManifestBuilder" Version="2.4.1" />
		<PackageReference Include="Pathoschild.Stardew.ModBuildConfig" Version="4.3.2" />
		<PackageReference Include="Pathoschild.Stardew.ModTranslationClassBuilder" Version="2.2.0" />
	</ItemGroup>

</Project>
